# Frontend Environment Variables Template
# Copy this file to .env.local for local development
# For production, set these in Vercel dashboard

# Backend API URL
# Local development: http://localhost:8080
# Production: https://your-backend.onrender.com
VITE_API_URL=http://localhost:8080

# WebSocket Server URL for real-time collaboration
# Local development: ws://localhost:1234
# Production: wss://your-collaboration-server.onrender.com
VITE_WEBSOCKET_URL=ws://localhost:1234
